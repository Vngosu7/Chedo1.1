def pr3(text):

  lines = text.split('\n')

  for line in lines:

      sys.stdout.write(line+'\n')

      sys.stdout.flush()

      sleep(0.1)

def pr(text):

  for i in range(len(text)+1):

      sys.stdout.write("\r" + text[:i])

      sys.stdout.flush()

      sleep(0.01)

  print()



